FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left_out
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    left_in
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    right_out
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    right_in
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    top_out
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top_in
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    bot_out
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    bot_in
    {
        type            fixedValue;
        value        uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


