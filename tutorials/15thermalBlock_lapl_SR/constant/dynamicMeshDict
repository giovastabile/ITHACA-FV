FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh   dynamicMotionSolverFvMesh;
motionSolverLibs ("libfvMotionSolvers.so");
twoDMotion yes;
solver displacementLaplacian;
frozenDiffusion no; 
diffusivity quadratic inverseDistance (left_in top_in right_in bot_in);


